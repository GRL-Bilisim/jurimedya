	 $(".resimsil").on('click',function(){

  // Holds the product ID of the clicked element

  
if (confirm('Resmi silmek istediğinizden eminmisiniz?İşlem geri alınamayacaktır')) 
    {
  

  	var idsi = $(this).attr('id');

	

	var temp = new Array();

	

	temp = idsi.split("_");
  
 document.getElementById("resim_"+temp[1]).src = "";
   document.getElementById(temp[1]).value = "";
   
 
   
   
   $.ajax({

      url: 'resimsil.php?menu_id=<?php echo $_GET[menu_id]; ?>&kayitid=<?php echo $_GET[kayitid]; ?>&alan='+temp[1],

      type: 'GET',

      data:'',

      processData: false,

      contentType: false,

	   success: function(data)

           {
               if (data=='OK')
               {
                   
                   location.reload();
               }
               
           }
           
        });
   
  
  
    }


});
	
	

