	     {
	    $("#degisenresim").attr('src','https://casewi.com/panel/imagen.php?urunsayisi=1&renk=renk1&model1=7&urunid=-1&name=İsim1&font=&renk2=&yon='+yon1+'&desen=&name2=İsim2&font2=&yon2='+yon2+'&top1='+top1+'&top2='+top2);
	    
	     }

	
		$('#ajax-modal').modal({

            show:true,

          
        });
	    
	}

	  if (ide=='Değiştir')

	{
	    
	    
	    $.ajax({

      url: 'kayitduzenle.php?kayitid='+temp[1]+'&page_id=kayitduzenle&menu_id=<?php echo $ozelformid; ?>',

