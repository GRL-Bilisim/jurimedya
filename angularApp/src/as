 $( '#password-change' ).on('click', function( ) {

	 

var eskisifre=$("#eskisifre").val();

var yenisifre=$("#yenisifre").val();

var yenisifre2=$("#yenisifre2").val();

if (yenisifre!=yenisifre2)

	{

		alert('Yeni şifreler uyuşmuyor');

	}	

	else

		{

$.ajax({

      url: 'sifredegistir.php?eskisifre='+eskisifre+'&yenisifre='+yenisifre,

      type: 'GET',

      data:'',

      processData: false,

      contentType: false,

	   success: function(data)

           {

			  

			  if (data=='OK')

			  {

				 alert('Şifre değişti, lütfen yeni şifrenizle tekrar giriş yapınız.'); 

				 

				 window.location.href ='index.php?page_id=cikisyap';

				  

			  }

			  else

			  {

	          alert('Şifre değişikliği yapılamadı, eski şifre hatalı.'); 

              }

           }

    });

	 }

	  



	  });




