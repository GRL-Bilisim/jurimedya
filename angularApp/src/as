           $sql="select sum(geneltoplam) as geneltoplami from siparisyonetimi";
                $sorgula=mysqli_query($connection,$sql);
                
                while ($okut=mysqli_fetch_assoc($sorgula))
                {
                $toplamtutar=number_format($okut[geneltoplami],2);      
               $toplamtutari=$okut[geneltoplami];      
               
                }
                
         $sql="select sum(geneltoplam) as geneltoplami from siparisyonetimi where optradio like 'Kap%'";
                $sorgula=mysqli_query($connection,$sql);
                
                while ($okut=mysqli_fetch_assoc($sorgula))
                {
                $kapidaodeme=number_format($okut[geneltoplami],2);       
                $kapidaoran=round(100*$okut[geneltoplami]/$toplamtutari);       
               
                }
                
                
                  $sql="select sum(geneltoplam) as geneltoplami from siparisyonetimi where optradio like 'Havale%'";
                $sorgula=mysqli_query($connection,$sql);
                
                while ($okut=mysqli_fetch_assoc($sorgula))
                {
                $havale=number_format($okut[geneltoplami],2); 
                 $havaleoran=round(100*$okut[geneltoplami]/$toplamtutari);   
                }
                
                
                 
                  $sql="select sum(geneltoplam) as geneltoplami from siparisyonetimi where optradio like 'Kredi%'";
                $sorgula=mysqli_query($connection,$sql);
                
                while ($okut=mysqli_fetch_assoc($sorgula))
                {
                $kredi=number_format($okut[geneltoplami],2);
                 $kredioran=round(100*$okut[geneltoplami]/$toplamtutari);  
                }
                
                
                   $sql="select sum(geneltoplam) as geneltoplami from siparisyonetimi where siparisdurumu like '%ptal%' or siparisdurumu like '%ade%'";
                $sorgula=mysqli_query($connection,$sql);
                
                while ($okut=mysqli_fetch_assoc($sorgula))
                {
                $iptaledilen=number_format($okut[geneltoplami],2);   
                 $iptaloran=round(100*$okut[geneltoplami]/$toplamtutari);  
                }
                
                   $sql="select sum(geneltoplam) as geneltoplami from siparisyonetimi where siparisdurumu like '%bekl%'";
