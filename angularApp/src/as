	     {
	    $("#degisenresim").attr('src','https://casewi.com/panel/imagen.php?urunsayisi=1&renk=renk1&model1=7&urunid=-1&name=Deneme&font=&renk2=&yon='+yon1+'&desen=&name2=&font2=&yon2=Y&top1='+top1+'&top2='+top2);
	    
	     }
	
