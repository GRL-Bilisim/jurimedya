		$(".btn").on('click',function(){



	var ide = $(this).attr('title');

	

	var idsi = $(this).attr('id');

	

	var temp = new Array();

	

	temp = idsi.split("_");
	

	  if (ide=='Önizle')

	{
	    
	    
	    var top1=$( "#yazikonumu").val();
	     var top2=$( "#yazikonum2").val();
