  $('#kirasuresi').change(function() {

			

		

    var sure=$(this).val();

	



	var myDate = new Date($("#kirayaverilis").val());

	

	

	var ms = myDate.getTime() + sure*86400000;

var tomorrow = new Date(ms);

	





	var y = tomorrow.getFullYear(),

    m = tomorrow.getMonth() + 1, // january is month 0 in javascript

    d = tomorrow.getDate();

var pad = function(val) { var str = val.toString(); return (str.length < 2) ? "0" + str : str};

dateString = [y, pad(m), pad(d)].join("-");

	





 $('#gerialinacaktarih').val(dateString);

 

  var gunlukbedel=$("#gunluk").val();

  var odenentutar=$("#odenentutar").val();

	

	

	if (gunlukbedel!="")

	{

	$('#anlasilantutar').val(gunlukbedel*sure);

	$('#kalantutar').val((gunlukbedel*sure)-odenentutar);

	

	

 	

	}

	

	

	

	

});





